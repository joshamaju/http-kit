{
  "name": "http-kit",
  "version": "0.0.1",
  "description": "Platform agnostic fetch kit for the Effect package",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --w",
    "clean": "rimraf dist",
    "prepublish": "clean && tsc"
  },
  "keywords": [
    "http",
    "fetch",
    "request",
    "response",
    "effect",
    "effect-ts",
    "xmlhttprequest"
  ],
  "author": "Joshua Amaju",
  "license": "ISC",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./fetch": {
      "import": "./dist/fetch/index.js",
      "types": "./dist/fetch/fetch.d.ts"
    },
    "./body": {
      "import": "./dist/body/index.js",
      "types": "./dist/body/index.d.ts"
    },
    "./function": {
      "import": "./dist/function.js",
      "types": "./dist/function.d.ts"
    },
    "./response": {
      "import": "./dist/response.js",
      "types": "./dist/response.d.ts"
    }
  },
  "devDependencies": {
    "@effect/data": "^0.12.10",
    "@effect/io": "^0.27.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@effect/data": "^0.12.10",
    "@effect/io": "^0.27.2"
  }
}
